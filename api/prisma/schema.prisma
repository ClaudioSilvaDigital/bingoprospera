generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Claim {
  id             String   @id @default(cuid())
  sessionId      String
  playerId       String
  playerName     String
  layout         Json
  marks          Json
  clientHasBingo Boolean
  serverCheck    String
  declaredAt     DateTime @default(now())

  @@index([sessionId])
  @@index([declaredAt])
}